<?php

use Drupal\Core\Form\FormStateInterface;
function landingpage_form_alter(&$form,  $form_state, $form_id) {
    kint($form);
    if ($form_id == 'node_page_form'){
        $form['new_field'] = [
            '#type' => 'textfield',
            '#title' => t('New Field'),
            '#default_value' => '',
            '#required' => FALSE,
          ];
    }
}
